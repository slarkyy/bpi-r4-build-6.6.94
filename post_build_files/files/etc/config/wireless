
config wifi-device 'radio0'
	option type 'mac80211'
	option path 'soc/11300000.pcie/pci0000:00/0000:00:00.0/0000:01:00.0'
	option radio '0'
	option band '2g'
	option channel '1'
	option htmode 'EHT20'
	option country 'GB'
	option cell_density '0'
	option txpower '20'
	option noscan '1'
	option rnr '1'
	option tx_burst '0.0'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'LuVa_2G'
	option encryption 'psk2'
	option disassoc_low_ack '0'
	option key 'c8HDe2HdWjS'
	option wnm_sleep_mode '1'
	option bss_transition '1'
	option ieee80211w '1'
	option ocv '0'
	option mbo '0'
        option wpa_disable_eapol_key_retries '1'
        option multicast_to_unicast '1'
        option ieee80211v '1'
        option ieee80211r '1'
        option mobility_domain 'a1b2'
        option ft_over_ds '1'
        option ft_psk_generate_local '1'


config wifi-device 'radio1'
	option type 'mac80211'
	option path 'soc/11300000.pcie/pci0000:00/0000:00:00.0/0000:01:00.0'
	option radio '1'
	option band '5g'
	option channel '104'
	option htmode 'EHT40'
	option country 'GB'
	option cell_density '0'
	option log_level '1'
	option noscan '1'
	option rnr '1'
	option background_radar '1'
	option tx_burst '0.0'

config wifi-iface 'default_radio1'
	option device 'radio1'
	option network 'lan'
	option mode 'ap'
	option ssid 'LuVa_5G'
	option encryption 'sae'
	option key 'c8HDe2HdWjS'
	option ieee80211k '1'
	option wnm_sleep_mode '1'
	option bss_transition '1'
	option ocv '0'
	option macaddr 'random'
	option mbo '1'
         option disassoc_low_ack '1'
        option wpa_disable_eapol_key_retries '1'
        option multicast_to_unicast '1'
        option ieee80211v '1'
        option ieee80211r '1'
        option mobility_domain 'a1b2'
        option ft_over_ds '1'
        option ft_psk_generate_local '1'


config wifi-device 'radio2'
	option type 'mac80211'
	option path 'soc/11300000.pcie/pci0000:00/0000:00:00.0/0000:01:00.0'
	option radio '2'
	option band '6g'
	option channel '69'
	option htmode 'EHT160'
	option country 'GB'
	option cell_density '0'
	option log_level '1'
	option tx_burst '0.0'
	option background_radar '1'
	option noscan '1'

config wifi-iface 'default_radio2'
	option device 'radio2'
	option network 'lan'
	option mode 'ap'
	option ssid 'LuVa_6G'
	option encryption 'sae'
	option key 'c8HDe2HdWjS'
	option ieee80211k '1'
	option wnm_sleep_mode '1'
	option bss_transition '1'
	option ocv '0'
	option macaddr 'random'
	option sae_pwe '2'
	option ieee80211w '2'
	option mbo '1'
        option disassoc_low_ack '1'
        option wpa_disable_eapol_key_retries '1'
        option multicast_to_unicast '1'
        option ieee80211v '1'
        option ieee80211r '1'
        option mobility_domain 'a1b2'
        option ft_over_ds '1'
        option ft_psk_generate_local '1'C

config wifi-mld 'ap_mld_1'
	option ifname 'ap-mld-1'
	option mld_id '1'
	option ssid 'LuVa'
	option encryption_rsno_2 'sae-ext'
	option key 'c8HDe2HdWjS'
	option sae_pwe '2'

